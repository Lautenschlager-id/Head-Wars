		<End>&#255;</End>
	  </CharacterRegion>
	  <CharacterRegion>
		<!--The arrows-->
		<Start>&#8592;</Start>
		<End>&#8595;</End>
	  </CharacterRegion>
	</CharacterRegions>
  </Asset>
</XnaContent>