	<FontName>Arial</FontName>
	<Size>12</Size>
	<Spacing>0</Spacing>
	<UseKerning>true</UseKerning>
	<Style>Bold</Style>
	<CharacterRegions>
	  <CharacterRegion>
		<!--If incompatible, use 32 - 255-->
